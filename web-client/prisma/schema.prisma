generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  userId        Int         @id @default(autoincrement())
  firstName     String      @db.VarChar(250)
  lastName      String      @db.VarChar(250)
  email         String      @db.VarChar(250)
  password      String      @db.VarChar(250)
  userType      String      @db.VarChar(250)
  isActive      String      @default("1") @db.Char(1)
  registeredAt  DateTime   @default(now())
}

model garages {
  garageId      Int         @id @default(autoincrement())
  garageName    String      @db.VarChar(250)
  servicesOffered String      @db.VarChar(250)
  Location    String      @db.VarChar(250)
  garagePhone   String      @db.VarChar(250)
  garageEmail   String      @db.VarChar(250)
  timeOpen      String      @db.VarChar(250)
  timeClose     String      @db.VarChar(250)
  addedBy       String      @db.VarChar(250)
  registeredAt  DateTime    @default(now())
}